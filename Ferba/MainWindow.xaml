                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Image
                        x:Name="ColorsBarPic"
                        Grid.Column="0"
                        Grid.Row="0"
                        Source="./Assets/Images/ColorsBar.png"
                                MouseDown="ColorsBarPic_MouseDown"/>
                            <Image
                        x:Name="BodyBarPic"
                        Grid.Column="1"
                        Grid.Row="0"
                        Source="./Assets/Images/BodyBar.png"
                                MouseDown="BodyBarPic_MouseDown"/>
                            <Image 
                        x:Name="MomentsBarPic"
                        Grid.Column="0"
                        Grid.Row="1"
                        Source="./Assets/Images/MomentsBar.png"
                                MouseDown="MomentsBarPic_MouseDown"/>
                            <Image 
                        x:Name="NatureBarPic"
                        Grid.Column="1"
                        Grid.Row="1"
                        Source="./Assets/Images/NatureBar.png"
                                MouseDown="NatureBarPic_MouseDown"/>
                        </Grid>
                    </TabItem>
                </TabControl>
            </Grid>
